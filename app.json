{
  "expo": {
    "name": "FuerteApache",
    "slug": "FuerteApache",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "config": {
        "googleMapsApiKey": "" 
     }
    },
    "android": {
        "config": {
          "googleMaps": { 
            "apiKey": "" 
          }   
       }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
